@page
@model FlightFinder.Pages.FlightsModel 
@{
    ViewData["Title"] = "Flights";
}

    <div class="wrapper">
        <b class="text-left">From</b>
        <img class="search-icon" src="data:image/svg+xml;utf8;base64," />
        <input class="from-search" placeholder="Airport, City or County" type="text">
        <b class="text-left">To</b>
        <img class="search-icon" src="data:image/svg+xml;utf8;base64," />
        <input class="to-search" placeholder="Airport, City, or Country" type="text">
        <b class="text-left">Departure</b>
        <img class="calendar-icon" src="~/css/images/calender.png" />
        <input type="date" class="search-date" placeholder="MM/DD/YYYY" />
        <b class="text-left">Arrival</b>
        <img class="calendar-icon" src="~/css/images/calender.png" />
        <input type="date" class="search-date" placeholder="MM/DD/YYYY" />
    </div>  

